
<!DOCTYPE html>
<meta charset="utf-8">
<style>

.subunit.SCT { fill: #ddc; }
.subunit.WLS { fill: #cdd; }
.subunit.NIR { fill: #cdc; }
.subunit.ENG { fill: #dcd; }

.subunit.IRL,
.subunit-label.IRL {
  display: none;
}

.subunit-boundary {
  fill: none;
  stroke: #777;
  stroke-dasharray: 2,2;
  stroke-linejoin: round;
}

.subunit-boundary.IRL {
  stroke: #aaa;
}

.subunit-label {
  fill: #777;
  fill-opacity: .5;
  font-size: 20px;
  font-weight: 300;
  text-anchor: middle;
}

.place,
.place-label {
  fill: #444;
}

text {
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 10px;
  pointer-events: none;
}

</style>
<body>
  <script src="https://d3js.org/d3.v7.min.js"></script>
<script src="https://lib.baomitu.com/topojson/3.0.2/topojson.min.js"></script>
<script>

var width = 960,
    height = 1160;

var projection = d3.geoAlbers()
    .center([0, 36])
    .rotate([-105, 0])
    .parallels([27, 45])
    // .scale(1200 * 5)
    .translate([width / 2, height / 2]);

var path = d3.geoPath()
    .projection(projection)
    .pointRadius(2);

var svg = d3.select("body").append("svg")
    .attr("width", width)
    .attr("height", height);

    // geo.path geoPath
d3.json("../map/cn.json").then(function(uk, error) {
  
  var subunits = topojson.feature(uk, uk.objects.subunits),
      places = topojson.feature(uk, uk.objects.places);


      svg.append("path")
      .datum(topojson.feature(uk, uk.objects.subunits))
      .attr("d", path);

});

</script>
